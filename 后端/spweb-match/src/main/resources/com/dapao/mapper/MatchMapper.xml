<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dapao.mapper.MatchMapper">

    <select id="page" resultType="com.dapao.pojo.Match">
        select *
        from `match`.`match1`
        <where>
            <if test="matchKeyValue != null and matchKeyValue != ''">
                name like concat('%', #{matchKeyValue}, '%')
            </if>
            <if test="begin != null">
                and begin >= #{begin}
            </if>
            <if test="end != null" >
                and #{end} >= end
            </if>
        </where>
    </select>
</mapper>